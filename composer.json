{
    "name": "simonmarcellinden/scriptloader",
    "description": "Package for load dynamically Scripts & Styles",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Marcel Linden",
            "email": "info@snerve.de"
        }
    ],
    "homepage": "https://github.com/simonmarcellinden/scriptloader",
    "keywords": ["Laravel", "ScriptLoader", "script", "style", "javasdscript", "stylesheet"],
    "require": {
        "illuminate/support": "~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "SimonMarcelLinden\\ScriptLoader\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimonMarcelLinden\\ScriptLoader\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SimonMarcelLinden\\ScriptLoader\\ScriptLoaderServiceProvider"
            ],
            "aliases": {
                "ScriptLoader": "SimonMarcelLinden\\ScriptLoader\\Facades\\ScriptLoader"
            }
        }
    }
}
